$(document).ready(function() {
   
    $('.navItem').css('color', 'white');

   
    $('#nav').css('background-color', '#145');

    
    $('li:even').css('background-color', '#ccc');

   
    $('li:odd').css('background-color', '#aaa');


    $('#myInput').val('David');
});
